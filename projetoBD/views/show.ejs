<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Banco de dados do condominio</title>
</head>
<body>
    <h3>Cod√¥minos</h3>
    <table border="1">
        <thead>
            <tr>
                <td>Nome</td>
                <td>Sobrenome</td>
                <td>celular</td>
                <td>CPF</td>
                <td>RG</td>
                <td>E-mail</td>
                <td>Complemento</td>
            </tr>
        </thead>
        <tbody>
            <% bancocond.forEach(function(details){ %>
                <tr>
                    <td><%= details.name %></td>
                    <td><%= details.surname %></td>
                    <td><%= details.celular %></td>
                    <td><%= details.cpf %></td>
                    <td><%= details.rg %></td>
                    <td><%= details.email %></td>
                    <td><%= details.complemento %></td>
                    <td><a href="/edit/<%= details._id %>">Editar</a> - <a href="/delete/<%= details._id %>">Deletar</a></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
    <a href="/"> <button type="submit">Registar</button> </a>
</body>
</html>